{
  "name": "rtm_transform",
  "version": "0.1.0",
  "description": "Small utilities to convert RememberTheMilk JSON exports into Todoist CSV imports",
  "main": "transform.js",
  "scripts": {
    "extract": "node extract_schema.js",
    "transform": "node transform.js",
  "test": "node ./scripts/ensure_sample_export.js && npx tap \"scripts/*.tap.js\" --no-coverage",
  "test:tap": "npx tap \"scripts/*.tap.js\" --no-coverage",
  "test:all": "npm run extract && npm run transform && npm run test:tap",
    "format": "node ./scripts/format_json.js"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "csv-parse": "^5.0.0"
  },
  "devDependencies": {
    "tap": "^16.2.0"
  }
}
